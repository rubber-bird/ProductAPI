  },
  "homepage": "https://github.com/rubber-bird/ProductAPI#readme",
  "dependencies": {
    "artillery": "^1.7.6",
    "bluebird": "^3.7.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "newrelic": "^7.5.1",
    "pg": "^8.6.0",
    "prom-client": "^13.1.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^7.29.0",
