{
  "name": "productapi",
  "version": "1.0.0",
  "description": "",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "NODE_ENV=production node --require dotenv/config ./src/index.js",
    "start:dev": "NODE_ENV=development nodemon --watch src --require dotenv/config ./src/index.js",
    "lint": "eslint ./src/**/*.js",
    "precommit-msg": "echo 'Precommit checks...' && exit 0",
    "docs": "jsdoc ./source/ -r -d docs -R ./README.md",
    "test": "jest",
    "stress:test": "artillery run --output report.json ./tests/products.yml",
    "stress:test:report": "artillery report report.json"
  },
  "pre-commit": [
    "precommit-msg",
    "lint"
  ],
  "repository": {
    "type": "git",
